{
  "name": "weex-index-demo",
  "version": "1.0.0",
  "description": "A weex project",
  "author": "",
  "private": true,
  "doctor": "node doctor.js",
  "scripts": {
    "start": "npm run serve",
    "build": "webpack --env.NODE_ENV=common",
    "build:dev": "cross-env NODE_ENV=production apiEnv=dev webpack --env.NODE_ENV=production",
    "build:gray": "cross-env NODE_ENV=production apiEnv=gray webpack --env.NODE_ENV=production",
    "build:prodold": "cross-env NODE_ENV=production apiEnv=prod ISHARMONY=false webpack --env.NODE_ENV=production",
    "build:harmony": "cross-env NODE_ENV=harmony apiEnv=prod ISHARMONY=true webpack --env.NODE_ENV=harmony",
    "build:prod": "npm run build:harmony && npm run build:prodold ",
    "lint": "eslint --ext .js,.vue src/pages/**/* src/utils src/components --fix",
    "serve": "webpack-dev-server --env.NODE_ENV=development --progress",
    "serve:harmony": "cross-env NODE_ENV=development apiEnv=dev ISHARMONY=true webpack-dev-server --env.NODE_ENV=development --env.ISHARMONY=true --env.apiEnv=dev --progress",
    "serve:all": "concurrently 'npm:start' 'npm:serve:harmony'",
    "ide:preview": "npm run start",
    "copy:ios": "mkdir -p platforms/ios/bundlejs/images/ && cp src/images/*.* platforms/ios/bundlejs/images/",
    "copy:android": "mkdir -p platforms/android/app/src/main/res/drawable-hdpi/ && cp src/images/*.* platforms/android/app/src/main/res/drawable-hdpi/",
    "ios": "npm run copy:ios && weex run ios",
    "android": "npm run copy:android && weex run android",
    "postinstall": "patch-package"
  },
  "keywords": [
    "weex"
  ],
  "license": "MIT",
  "dependencies": {
    "@antv/f2": "^3.3.9",
    "@babel/polyfill": "^7.12.1",
